{
  "name": "chrome-extension-network-hook",
  "version": "1.0",
  "minimum_chrome_version": "10.0",
  "description": "chrome extension example",
  "devtools_page": "index.html",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "manifest_version": 2
}
